{
  "name": "zoom-control-center",
  "version": "1.0.0",
  "description": "Comprehensive Zoom Management Console for Teams",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "client": "npm --prefix client run dev",
    "install:all": "npm install && npm --prefix client install",
    "build": "npm --prefix client run build",
    "start": "node server/index.js",
    "start:prod": "node --input-type=module -e \"process.env.NODE_ENV='production'; await import('./server/index.js');\""
  },
  "dependencies": {
    "@azure/identity": "^4.13.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.14.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
